{
  "name": "pages",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/pages/src",
  "prefix": "lda",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/pages"],
      "options": {
        "jestConfig": "libs/pages/jest.config.ts",
        "passWithNoTests": true,
        "assets": ["src/lib/assets/images/favicon.ico", "src/lib/assets"],
        "styles": [
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "src/lib/assets/scss/style.scss"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/pages/**/*.ts", "libs/pages/**/*.html"]
      }
    }
  },
  "tags": []
}
